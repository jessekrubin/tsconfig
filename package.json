{
  "name": "@jsse/tsconfig",
  "type": "module",
  "version": "0.2.4",
  "description": "tsconfig",
  "author": "jessekrubin <jessekrubin@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jawascript",
    "typescript",
    "tsconfig",
    "config"
  ],
  "exports": {
    ".": "./tsconfig.json",
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json",
    "./tsconfig.cjs.json": "./tsconfig.cjs.json",
    "./tsconfig.strict-cjs.json": "./tsconfig.strict-cjs.json",
    "./tsconfig.strict.json": "./tsconfig.strict.json",
    "./cjs": "./tsconfig.cjs.json",
    "./cjs.json": "./tsconfig.cjs.json",
    "./strict-cjs": "./tsconfig.strict-cjs.json",
    "./strict-cjs.json": "./tsconfig.strict-cjs.json",
    "./strict": "./tsconfig.strict.json",
    "./strict.json": "./tsconfig.strict.json",
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "jsse-tsconfig": "./dist/cli.js"
  },
  "files": [
    "dist",
    "package.json",
    "tsconfig.cjs.json",
    "tsconfig.json",
    "tsconfig.strict-cjs.json",
    "tsconfig.strict.json"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm run gen",
    "gen": "tsx scripts/update-pkg-json-exports.mts",
    "check": "eslint . && prettier --check .",
    "fmt": "prettier --write .",
    "fmtc": "prettier --check .",
    "lint": "eslint .",
    "test": "vitest --run",
    "tsc": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@jsse/eslint-config": "^0.3.0",
    "@swc/core": "^1.10.18",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6",
    "zx": "^8.3.2"
  }
}
